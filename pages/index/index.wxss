.header{
  width: 100%;
  height: 400rpx;
  background: radial-gradient(#fe2b45,#d23c47)
}
.color-white {
  color: white;
}
.link{
  margin-top: 40rpx;
  color: white;
  text-decoration: underline;
  font-size: 14px;
}
.cut-off{
  padding: 10rpx;
  border-bottom: 10rpx solid #f3f3f3;
}

.content{
  padding: 20rpx;
  border-bottom: 10rpx solid #f3f3f3;
}
.line{
  border-top: 2px solid gray;
  width: 200rpx;
  height: 0;
}
