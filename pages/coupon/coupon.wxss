.container{
  width: 100%;
  height: 100vh;
  background: #efeff4;  
}
.icon{
  background: #d6d6da;
  color: white;  
  padding: 40rpx 30rpx 20rpx 40rpx;
  border-radius: 50%;
}
.margin{
  margin: 40rpx 0;  
}
.btn{
  background: #f8f8f8;
  padding: 10rpx 30rpx;  
  border-radius: 5px;
  color: black;
}
.noCoupon{
  margin-bottom: 200rpx;  
}
.card{
  border: 1px solid #47c1c4;
  width: 90%;
  height: 200rpx;
  flex-wrap: nowrap;
  background: white;
  color: #47c1c4;
  margin-top: 40rpx;
  border-radius: 5px;
}
.leftContent{
  width: 300rpx;
}
.margin-right{
  margin-right: 20rpx;
}
.couponType{
  background: #47c1c4;
  color: white;
  padding: 10rpx 20rpx;
}
